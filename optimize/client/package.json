{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "homepage": ".",
  "dependencies": {
    "@bpmn-io/element-templates-icons-renderer": "^0.3.0",
    "@camunda/camunda-composite-components": "^0.6.2",
    "@camunda/camunda-optimize-composite-components": "^0.0.16",
    "@carbon/icons-react": "^11.41.0",
    "@carbon/react": "^1.55.0",
    "@lexical/react": "^0.15.0",
    "@ungap/structured-clone": "^1.2.0",
    "bpmn-js": "17.9.2",
    "bpmn-js-disable-collapsed-subprocess": "^0.1.7",
    "chart.js": "^4.4.0",
    "chartjs-plugin-datalabels": "^2.2.0",
    "classnames": "^2.3.2",
    "date-fns": "^3.0.0",
    "debounce": "^2.0.0",
    "fast-deep-equal": "^3.1.3",
    "fitty": "^2.3.7",
    "focus-visible": "^5.2.0",
    "heatmap.js": "^2.0.5",
    "immutability-helper": "^3.1.1",
    "lexical": "^0.15.0",
    "react": "17.0.2",
    "react-date-range": "^2.0.0-alpha.3",
    "react-dom": "17.0.2",
    "react-full-screen": "^1.1.1",
    "react-grid-layout": "^1.4.1",
    "react-resizable": "^3.0.5",
    "react-router": "5.3.4",
    "react-router-dom": "5.3.4",
    "react-table": "^7.8.0",
    "use-deep-compare-effect": "^1.8.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts --expose-gc --max_old_space_size=1024 --no-compilation-cache test --verbose=false",
    "test:ci": "CI=true react-scripts --expose-gc --max_old_space_size=1024 --no-compilation-cache test --maxWorkers=$LIMITS_CPU --ci --testResultsProcessor='jest-junit'",
    "screenshots": "testcafe chrome e2e/tests/*.js -s thumbnails=false,path=../../camunda-platform-docs/optimize/components/userguide",
    "e2e": "testcafe -c 3 chrome e2e/sm-tests/*.js --disable-screenshots",
    "e2e:ci:headless": "testcafe -c 3 chrome:headless e2e/sm-tests/*.js -e --disable-screenshots --assertion-timeout 40000 -q attemptLimit=3,successThreshold=1",
    "e2e:ci:cloud:headless": "testcafe chrome:headless e2e/cloud-tests/smokeTest.js -e",
    "e2e:ci:sm:headless": "testcafe -c 3 firefox:headless e2e/sm-tests/*.js -e --disable-screenshots --assertion-timeout 40000 -q attemptLimit=3,successThreshold=1",
    "e2e:cloud": "testcafe chrome e2e/cloud-tests/smokeTest.js",
    "eject": "react-scripts eject",
    "postinstall": "node scripts/wireModules",
    "start-backend": "node scripts/start-backend",
    "start-backend-cloud": "node scripts/start-backend cloud",
    "restart-backend": "curl http://localhost:8100/api/restartBackend",
    "dependencies": "node scripts/dependencies.js",
    "prepare": "cd .. && husky client/.husky"
  },
  "lint-staged": {
    "src/**/*.scss": [
      "prettier --write"
    ],
    "src/**/*.{js,jsx}": [
      "eslint --fix --max-warnings=0",
      "prettier --write"
    ]
  },
  "jest-junit": {
    "suiteName": "client",
    "outputDirectory": ".",
    "outputName": "jest-test-results.xml",
    "suiteNameTemplate": "{filename}",
    "classNameTemplate": "client.{filename}",
    "titleTemplate": "{title}",
    "ancestorSeparator": " @ "
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@types/enzyme": "3.10.18",
    "@types/enzyme-adapter-react-16": "1.0.9",
    "@types/jest": "29.5.12",
    "@types/node": "20.16.1",
    "@types/react": "^18.0.11",
    "@types/react-date-range": "1.4.9",
    "@types/react-dom": "^18.0.11",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-table": "7.7.20",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.8.0",
    "ansi-html": "^0.0.9",
    "chalk": "^5.3.0",
    "date-fns": "^3.0.0",
    "dotenv": "^16.3.1",
    "enzyme": "3.11.0",
    "eslint-plugin-camunda-licensed": "^0.4.6",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-testcafe": "^0.2.1",
    "event-source-polyfill": "^1.0.31",
    "http-proxy-middleware": "^3.0.0",
    "husky": "^9.0.0",
    "jest-enzyme": "^7.1.2",
    "jest-junit": "^16.0.0",
    "license-checker": "^25.0.1",
    "lint-staged": "^15.2.2",
    "mock-socket": "^9.3.1",
    "node-fetch": "^2.6.7",
    "old-bpmn-js": "npm:bpmn-js@17.9",
    "opn": "^6.0.0",
    "prettier": "^3.3.0",
    "react-scripts": "5.0.1",
    "sass": "^1.77.2",
    "styled-components": "^6.1.9",
    "testcafe": "^3.3.0",
    "testcafe-browser-provider-browserstack": "^1.15.1",
    "tree-kill": "^1.2.2",
    "typescript": "^5.1.6",
    "ws": "^8.14.1",
    "xml2js": "^0.6.2"
  },
  "resolutions": {
    "**/jpeg-js": "^0.4.0",
    "**/kind-of": "^6.0.3"
  },
  "jest": {
    "resetMocks": false,
    "transformIgnorePatterns": [
      "/node_modules/(?!@camunda/camunda-.*?)"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "<rootDir>/src/__mocks__/styleMock.js"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
